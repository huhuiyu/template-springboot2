<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.huhuiyu.springboot2.dao.TbAuthUserInfoDAO">

    <select id="queryByKey" parameterType="java.lang.Integer" resultType="TbAuthUserInfo">
        select * from tb_auth_user_info
        where user_id = #{userId}
    </select>

</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.huhuiyu.springboot2.dao.TbAuthRoleDAO">
    <select id="queryByKey" parameterType="java.lang.Integer" resultType="TbAuthRole">
        select * from tb_auth_role
        where role_id = #{roleId}
    </select>
</mapper>